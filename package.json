  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Klemen Oslaj",
  "keywords": [
    "logger",
    "typescript",
    "ts-logger"
  ],
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.2",
    "html-webpack-plugin": "^2.30.1",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
